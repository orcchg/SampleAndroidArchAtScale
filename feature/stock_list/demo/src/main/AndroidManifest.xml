<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.orcchg.sample.atscale.stocklist.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/stock_ic_launcher"
        android:label="@string/stock_app_name"
        android:name=".App"
        android:roundIcon="@mipmap/stock_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApp">

        <activity android:name=".ui.StockListDemoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>